---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom" xmlns:webfeeds="http://webfeeds.org/rss/1.0">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <webfeeds:icon>https://degreeless.marketing/safari-pinned-tab.svg</webfeeds:icon>
    <webfeeds:logo>https://degreeless.marketing/safari-pinned-tab.svg</webfeeds:logo>
    <webfeeds:accentColor>da532c</webfeeds:accentColor>
    <webfeeds:related layout="card" target="browser"/>
    <atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>

    <generator>Jekyll v{{ jekyll.version }}</generator>
{% assign timeframe = 604800 %}

      <item>
        <title>Books</title>
        <description>
          {% for post in site.books %}
            {% assign post_in_seconds = post.date | date: "%s" | plus: 0 %}
              {% assign recent_posts = "now" | date: "%s" | minus: timeframe  %}
              {% if post_in_seconds > recent_posts %}
        &lt;h2&gt;{{ post.title | xml_escape }}&lt;/h2&gt;
        &lt;a href=&apos;{{ post.url | prepend: site.baseurl | prepend: site.url | xml_escape }}&apos; target=&apos;_blank&apos; style=&apos;color:#3399ff&apos;&gt;&lt;u&gt;View in Browser&lt;/u&gt;&lt;/a&gt;
        &lt;br /&gt;
        {{ post.content | xml_escape }}
        &lt;hr&gt;
          {% endif %}
        {% endfor %}
        </description>
        {% for post in site.posts limit:1 %}
        <pubDate>{{ post.date | date_to_rfc822 }}</pubDate>
        {% endfor %}
        <link>{{ "/books" | prepend: site.baseurl | prepend: site.url }}</link>
      </item>

      <item>
        <title>Podcasts</title>
        <description>
        &lt;a href=&apos;{{ "/podcasts" | prepend: site.baseurl | prepend: site.url }}&apos; target=&apos;_blank&apos; style=&apos;color:#3399ff&apos;&gt;&lt;u&gt;View all Podcasts&lt;/u&gt;&lt;/a&gt;
        &lt;br &gt;&lt;br &gt;

        {% assign sorted = site.data.podcasts | sort: 'date' | reverse %} 
        {% for item in sorted %}
                &lt;a href=&apos;{{ item.link | xml_escape }}&apos; target=&apos;_blank&apos;&gt;&lt;strong&gt;&lt;u&gt;{{ item.title | xml_escape }} →&lt;/u&gt;&lt;/strong&gt;&lt;/a&gt;
                &lt;br /&gt;
                {{ item.description | xml_escape }}
                &lt;br /&gt;
                &lt;hr&gt;
        {% endfor %}
        </description>
        {% for recent in sorted limit:1 %}
        <pubDate>{{ recent.date | date_to_rfc822 }}</pubDate>
        {% endfor %}
        <link>{{ "/podcasts" | prepend: site.baseurl | prepend: site.url }}</link>
      </item>


      <item>
        <title>Courses</title>
        <description>
        &lt;a href=&apos;{{ "/courses" | prepend: site.baseurl | prepend: site.url }}&apos; target=&apos;_blank&apos; style=&apos;color:#3399ff&apos;&gt;&lt;u&gt;View all Courses&lt;/u&gt;&lt;/a&gt;
        &lt;br &gt;&lt;br &gt;

        {% assign sorted = site.data.courses | sort: 'date' | reverse %} 
        {% for item in sorted %}
                &lt;a href=&apos;{{ item.link | xml_escape }}&apos; target=&apos;_blank&apos;&gt;&lt;strong&gt;&lt;u&gt;{{ item.title | xml_escape }} →&lt;/u&gt;&lt;/strong&gt;&lt;/a&gt;
                &lt;br /&gt;
                {{ item.description | xml_escape }}
                &lt;br /&gt;
                &lt;hr&gt;
        {% endfor %}
        </description>
        {% for recent in sorted limit:1 %}
        <pubDate>{{ recent.date | date_to_rfc822 }}</pubDate>
        {% endfor %}
        <link>{{ "/courses" | prepend: site.baseurl | prepend: site.url }}</link>
      </item>



      <item>
        <title>Guides</title>
        <description>
        &lt;a href=&apos;{{ "/guides" | prepend: site.baseurl | prepend: site.url }}&apos; target=&apos;_blank&apos; style=&apos;color:#3399ff&apos;&gt;&lt;u&gt;View all Guides&lt;/u&gt;&lt;/a&gt;
        &lt;br &gt;&lt;br &gt;

        {% assign list = site.data.guides | sort: 'date' | reverse %} 
        {% for topic in list %}
            {% for topicContent in topic %}
                {% for item in topicContent[1] %}
                    &lt;a href=&apos;{{ item["link"] | xml_escape }}&apos; target=&apos;_blank&apos;&gt;&lt;strong&gt;&lt;u&gt;{{ item["title"] | xml_escape }} →&lt;/u&gt;&lt;/strong&gt;&lt;/a&gt;
                    &lt;br /&gt;
                    {{ item["description"] | xml_escape }}
                    &lt;br /&gt;
                    &lt;hr&gt;
                {% endfor %}
            {% endfor %}
        {% endfor %}
        </description>
        {% for recent in sorted limit:1 %}
        <pubDate>{{ recent.date | date_to_rfc822 }}</pubDate>
        {% endfor %}
        <link>{{ "/guides" | prepend: site.baseurl | prepend: site.url }}</link>
      </item>


     <item>
        <title>Newsletters</title>
        <description>
        &lt;a href=&apos;{{ "/newsletters" | prepend: site.baseurl | prepend: site.url }}&apos; target=&apos;_blank&apos; style=&apos;color:#3399ff&apos;&gt;&lt;u&gt;View all Newsletters&lt;/u&gt;&lt;/a&gt;
        &lt;br &gt;&lt;br &gt;

        {% assign sorted = site.data.newsletters | sort: 'date' | reverse %} 
        {% for item in sorted %}
                &lt;a href=&apos;{{ item.link | xml_escape }}&apos; target=&apos;_blank&apos;&gt;&lt;strong&gt;&lt;u&gt;{{ item.title | xml_escape }} →&lt;/u&gt;&lt;/strong&gt;&lt;/a&gt;
                &lt;br /&gt;
                {{ item.description | xml_escape }}
                &lt;br /&gt;
                &lt;hr&gt;
        {% endfor %}
        </description>
        {% for recent in sorted limit:1 %}
        <pubDate>{{ recent.date | date_to_rfc822 }}</pubDate>
        {% endfor %}
        <link>{{ "/courses" | prepend: site.baseurl | prepend: site.url }}</link>
      </item>

  </channel>
</rss>
